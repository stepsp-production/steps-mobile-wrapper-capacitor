{
  "name": "steps-presinter-mobile",
  "version": "1.0.0",
  "private": true,
  "description": "Capacitor wrapper for Steps Presinter HLS + LiveKit web app",
  "scripts": {
    "init": "npm i && npx cap sync",
    "sync": "npx cap sync",
    "clean": "rm -rf node_modules ios android",
    "add:ios": "npx cap add ios",
    "add:android": "npx cap add android",
    "open:ios": "npx cap open ios",
    "open:android": "npx cap open android"
  },
  "dependencies": {
    "@capacitor/core": "^6.1.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.0",
    "@capacitor/ios": "^6.1.0",
    "@capacitor/android": "^6.1.0"
  }
}